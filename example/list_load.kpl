
// Use linked list functions

list_fns : "./list.kpl"\load

list : list_fns`list_new()

1 .. 10 @ { `counter; list_fns`list_push(list; counter) }

list_fns`list_each(list; { I64`value; U64`idx
    .io.colors.strings [ `reset; `blue; `green; `bold ]
    .io`stdout(String (
        blue
        "idx: "
        bold
        String $ idx
        clear
        green
        ", value: "
        bold
        String $ value
        reset
        "\n"
    ))
})
