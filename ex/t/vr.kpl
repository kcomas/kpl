
// vector example

f:FN(VD)${()
    v:VR[SG]$["a";"b"]
    $(FD;1) << ,(" ";v),"\n"
    v + "c"
    v + "d"
    $(FD;1) << ,(" ";v),"\n"
    [d;e]'-v
    -v
    b:'-v
    $(FD;1) << d," and ",b," popped \n"
    $(FD;1) << ,(" ";v),"\n"
    // errors throw
    // -v; -v
}

f();
n:I6$0
v:VR[SG]$[]
#@ (n < 5) {
    v + SG$n * 2
    n: n + 1
    [x;e]'v(n - 1)
    $(FD;1) << " ",["Catch:";x;",Ignore:";'v(n - 1);"\n"]
}

//v(3):"Hello World"

pv:FN(VD)${() $(FD;1) << ,(" ",v;"\n") }

pv()
