
// vector example

f:FN(VD)${()
    v:VR[SG]$["a";"b"]
    $(FD;1) << ,(" ";v),"\n"
    v + "c"
    v + "d"
    $(FD;1) << ,(" ";v),"\n"
    [d;e]'-v
    -v
    b:-v
    $(FD;1) << d," and ",b," popped \n"
    $(FD;1) << ,(" ";v),"\n"
    // errors throw
    // -v; -v
}

f();

v:VR[U6]$[]

n:U6$0

#@ (n < 5) {
    v + n
    $(FD;1)<<$(SG;v(n)),"\n"
    n: n + 1
}
