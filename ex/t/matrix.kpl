
m:VR[VR[F6]]$[VR[F6]$[0.1;0.2;0.3];VR[F6]$[1.1;1.2;1.3];VR[F6]$[2.1;2.2;3.3]]

g:FN(U6;U6;ER(F6))${(y;x)
    t:'m(y)
    [v;e]'t(x)
    #? (BL$e) { 'e }
    v
}

y:U6$0
#@ (y < 3) {
    x:U6$0
    #@ (x < 3) {
        $(FD;1) << " ",[" ";SG$'g(y;x);" "]
        x: x + 1
    }
    y: y + 1
    $(FD;1) << "\n"
}
